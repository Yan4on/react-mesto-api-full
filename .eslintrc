{
  "env": {
      "browser": true,
      "es2021": true,
      "node" : true
  },
  "extends": [
      "eslint:recommended",
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
    
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "linebreak-style": 0
  }
}